{
    "name": "@uppercod/css-to-object",
    "version": "1.1.1",
    "description": "css-to-json is a package that transforms the syntax from css to JSON format using regular expressions",
    "type": "module",
    "main": "./css-to-object.cjs",
    "module": "./src/css-to-object.js",
    "exports": {
        "import": "./src/css-to-object.js",
        "require": "./css-to-object.cjs"
    },
    "typings": "css-to-object.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/css-to-object.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/css-to-object/issues"
    },
    "homepage": "https://github.com/UpperCod/css-to-object#readme",
    "devDependencies": {
        "ava": "^3.10.1",
        "rollup": "^2.21.0"
    },
    "ava": {
        "files": [
            "test/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
